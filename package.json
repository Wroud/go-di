{
  "name": "go-di",
  "version": "1.4.1",
  "description": "Dependency injection",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "lib/index.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --opts mocha.opts && npm run size",
    "size": "size-limit",
