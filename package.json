    "publish:alpha": "npm run build & npm publish -tag alpha",
    "publish:rc": "npm run build & npm publish -tag rc",
    "publish:latest": "npm run build & npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wroud/go-di.git"
  },
  "release": {
    "branch": "master"
  },
  "keywords": [
    "di",
    "typescript"
  ],
  "author": "Wroud",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.4",
    "@types/chai": "^4.2.1",
    "@types/chai-arrays": "^1.0.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {}
}
